{
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,

    "go.formatOnSave": true,
    "go.buildOnSave": true,
    "go.lintOnSave": true,
    "go.lintTool": "golint",
    "go.vetOnSave": true,
    "go.buildFlags": ["-i"],
    "go.lintFlags": [],
    "go.vetFlags": [],
    "go.coverOnSave": false,
    "go.useCodeSnippetsOnFunctionSuggest": false,
    "go.goroot": "/usr/local/go",
    "go.gopath": "/Users/bill/code/go",
    "go.formatTool": "goimports",
    "go.gocodeAutoBuild": true,
    "go.enableCodeLens": {"references": false, "runtest": false},

    "editor.fontSize": 14,
    "editor.formatOnType": true,
    "editor.mouseWheelZoom": false,
    "editor.rulers": [80,120],
    "editor.trimAutoWhitespace": true,
    "editor.minimap.enabled": false,

    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/dist/public": true,
        "**/tmp": true,
        "**/tags": true,
        "**/public/dist": true,
        "**/.vscode": true
    },
    "files.trimTrailingWhitespace": false,

    "spellchecker.documentTypes": [
        "markdown",
        "latex",
        "plaintext"
    ],

    "spellchecker.ignoreFileExtensions": [
        ".go",
        ".cfg",
        ".sh"
    ]
,
"window.zoomLevel": -1,
"editor.wordWrap": "off",
"workbench.colorTheme": "Zenburn",
"workbench.colorCustomizations": {"editorLineNumber.foreground": "#9F9F9F"}
}